					ImGui_DoWindowEnd();
		
					if (!ImGui_TimerExists("ShutdownTimer"))
					{
						ImGui_AddTimer("ShutdownTimer", 4);
					}
					if (ImGui_TimerOver("ShutdownTimer"))
					{
