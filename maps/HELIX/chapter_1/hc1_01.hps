					ImGui_DoWindowStart("Shutdown", sdwindow, ImGui_NrmPosGroup(cVector3f((1.f-0.6)*0.5, (1.f-0.2)*0.5, 1.5)), ImGui_NrmSizeGroup(cVector2f(0.6, 0.2)));

