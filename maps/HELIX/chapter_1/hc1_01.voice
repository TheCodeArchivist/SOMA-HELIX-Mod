   <Line CharacterId="15" Prio="0">
    <Sound Text="Aleksey." Volume="0.6" FileName="ivashkin_3_crewmember_2_001_ivashkin_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="15" Prio="0">
