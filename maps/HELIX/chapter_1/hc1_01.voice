  <Subject Name="3_DatamineArkCrewMember3" UseSingleRandomLine="False" SceneId="5" EffectId="-1">
   <Line CharacterId="25" Context="Pedersen is &quot;peacefully&quot; dying. Lindwall and Hill there to comfort, gets into an argument." Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Can't we do anything for him?" ExtraEffectFile="Flashback/Level/04_02/pederson_blackbox" EndPadding="-1" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember3_001_lindwall_001" Timestamp="2014-09-02 11:31" />
   </Line>
   <Line CharacterId="26" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="No. Were out of sedatives. If we had a gun, this would be the time to pull it out." EndPadding="-0.35" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember3_002_hill_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="25" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Did not expect it to end like this. Should have jumped the climber back to Omicron." EndPadding="-0.6" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember3_003_lindwall_001" Timestamp="2015-04-10 02:53" />
   </Line>
   <Line CharacterId="24" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Without the ARK?" EndPadding="-0.2" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember3_004_hill_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="25" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Forget about the ARK." EndPadding="-0.1" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember3_005_lindwall_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="26" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="No, you don't get to say that. It's the only thing left of value. You know that." EndPadding="-0.4" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember3_006_hill_001" Timestamp="2014-09-02 11:31" />
   </Line>
   <Line CharacterId="25" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Yeah, yeah." EndPadding="-0.8" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember3_007_lindwall_001" Timestamp="2015-04-10 02:53" />
   </Line>
   <Line CharacterId="26" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Promise you won't leave it behind." EndPadding="-1.2" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember3_008_hill_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="25" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="We'll all be dead one day, then what?" EndPadding="-0.5" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember3_009_lindwall_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="26" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Well, better keep it safe until then. Don't want the WAU to take it; swallow it whole." EndPadding="-0.9" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember3_010_hill_001" Timestamp="2015-05-22 05:18" />
