   </Line>
   <Line CharacterId="15" Prio="0">
    <Sound Text="- sey" FileName="ivashkin_3_crewmember_2_003_ivashkin_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="15" Prio="0">
    <Sound Text="Lyoshka." FileName="ivashkin_3_crewmember_2_004_ivashkin_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="15" Prio="0">
    <Sound Text="My boy." FileName="ivashkin_3_crewmember_2_005_ivashkin_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="15" Prio="0">
    <Sound Text="Aleksey." FileName="ivashkin_3_crewmember_2_006_ivashkin_001" Timestamp="2014-08-22 07:16" />
