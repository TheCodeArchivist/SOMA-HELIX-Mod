  <Subject Name="4_KillLastHuman_ReEnter" UseSingleRandomLine="True" SceneId="4" EffectId="-1">
   <Line CharacterId="10" Prio="0" PlayOnce="True">
    <Sound Text="Thank you, for staying." Volume="0.7" FileName="lindwall_4_killlasthuman_reenter_001_lindwall_001" Timestamp="2014-08-18 01:20" />
   </Line>
   <Line CharacterId="10" Prio="0" PlayOnce="True">
    <Sound Text="Please stay." Volume="0.7" FileName="lindwall_4_killlasthuman_reenter_002_lindwall_001" Timestamp="2014-08-18 01:20" />
   </Line>
   <Line CharacterId="10" Prio="0" PlayOnce="True">
    <Sound Text="Please, don't leave. Not yet." Volume="0.7" FileName="lindwall_4_killlasthuman_reenter_003_lindwall_001" Timestamp="2014-08-18 01:20" />
   </Line>
  </Subject>
  <Subject Name="4_KillLastHuman_End_Staying" UseSingleRandomLine="False" SceneId="4" EffectId="-1">
   <Line CharacterId="10" Prio="0" PlayOnce="True">
    <Sound Text="Simon, you still there?" VoiceOffset="1.5" Volume="0.95" FileName="lindwall_4_killlasthuman_end_staying_001_lindwall_001" Timestamp="2015-05-22 05:18" />
   </Line>
   <Line CharacterId="1" Prio="0" PlayOnce="True">
    <Sound Text="I'm here, Sarah." VoiceOffset="0.25" FileName="lindwall_4_killlasthuman_end_staying_002_player_001" Timestamp="2014-08-18 05:01" />
   </Line>
   <Line CharacterId="10" Prio="0" PlayOnce="True">
    <Sound Text="Don't let them die, okay? Send them out there. To the stars." VoiceOffset="0.5" Volume="0.85" FileName="lindwall_4_killlasthuman_end_staying_003_lindwall_001" Timestamp="2014-09-02 11:31" />
   </Line>
  </Subject>
  <Subject Name="3_DatamineArkCrewMember1" UseSingleRandomLine="False" SceneId="5" EffectId="-1">
   <Line CharacterId="26" Direction="crying" Context="Hill is dying from malnourishment/organ failure. Lindwall is there to comfort." Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="What the fuck did we do?" ExtraEffectFile="Flashback/Level/04_02/hills_blackbox" EndPadding="-0.6" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember1_001_hill_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="25" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Hey, try not to think about it." EndPadding="-0.7" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember1_002_lindwall_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="26" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="My whole life. I've been a good man. We messed up, Sarah." EndPadding="-0.8" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember1_003_hill_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="25" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="There, there." EndPadding="-0.8" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember1_004_lindwall_001" Timestamp="2015-05-22 05:18" />
   </Line>
   <Line CharacterId="26" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Should have launched. Taken the chance." EndPadding="-0.35" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember1_005_hill_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="25" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="There's time. Ivashkin says we could try again, when things calm down." EndPadding="-0.5" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember1_006_lindwall_001" Timestamp="2014-09-02 11:31" />
   </Line>
   <Line CharacterId="26" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="You got to do it, you know? You got to." Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember1_007_hill_001" Timestamp="2014-09-02 11:31" />
   </Line>
   <Line CharacterId="26" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="*cough*" EndPadding="0.6" Volume="0.65" ReverbVolume="0" FileName="datamine_3_dataminearkcrewmember1_008_hill_001" Timestamp="2014-08-22 07:16" />
   </Line>
   <Line CharacterId="25" Prio="0" ChangeSource="True" EntitySource="Player">
    <Sound Text="Sssh..." Volume="0.65" ReverbVolume="0" EndsAfterExtraEffect="True" FileName="datamine_3_dataminearkcrewmember1_009_lindwall_001" Timestamp="2015-05-22 05:18" />
   </Line>
  </Subject>
